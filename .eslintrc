{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "arrow-parens": [
            "error",
            "always"
        ],
        "camelcase": 0,
        "no-unused-vars": 0,
        "max-len": 0,
         "func-names": 0,
         "no-console": 0,
    },
    "env": {
        "jest": true
    }
}
